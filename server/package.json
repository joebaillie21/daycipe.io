{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:ci": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js --runInBand --forceExit",
    "test:integration:ci": "jest --config jest.integration.config.js --coverage --runInBand --forceExit"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testMatch": ["**/test/unit/**/*.test.js"],
    "collectCoverageFrom": [
      "routes/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "pg": "^8.13.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@faker-js/faker": "^9.7.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.0"
  }
}
